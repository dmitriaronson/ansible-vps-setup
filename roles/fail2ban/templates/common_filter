[INCLUDES]
before = common.conf

[Definition]
ignoreregex =
failregex = ^.*Username or password is incorrect\. Try again\. IP: <ADDR>\. Username:.*$
            ^.*Invalid admin token\. IP: <ADDR>.*$
            ^<HOST> -.*"(GET|POST|HEAD).* HTTP.*" (4[0-9]{2}|5[0-9]{2})
            .*user .* was not found in .*access log.*
            .*(Failed authentication attempt|invalid credentials|Attempted access of unknown user).* from <HOST>
